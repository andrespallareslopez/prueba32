{
  "name": "prueba32",
  "version": "1.0.0",
  "description": "prueba32 project",
  "main": "prueba29.js",
  "scripts": {
    "start-prod": "concurrently --kill-others \"npm run liveserver\" \"npm run sass-watch\"  \"npm run build:watchify2\" \"npm run minify-brify\" \"npm run bundle-sass\" \"npm run bundle-css\" ",
    "start-dev": "concurrently --kill-others \"npm run liveserver\" \"npm run sass-watch\"  \"npm run build:watchify2\" \"npm run copy-brify\" \"npm run bundle-sass\" \"npm run bundle-css\" ",
    "uglify-js": "uglifyjs bundle-brify.js -o dist/minify/bundle-brify.min.js --compress",
    "copy-bundle": "gulp copy-bundle",
    "bundle-parcelify": "npm run build:browserify2",
    "sass-watch": "sass -w src/shared/js/node_modules/**",
    "bundle-sass": "onchange --no-exclude \"./src/shared/js/node_modules/**/*.css\" -v -- npm run bundle-parcelify",
    "copy-brify": "onchange ./bundle-brify.js -v -- npm run copy-bundle",
    "minify-brify": "onchange ./bundle-brify.js -v -- npm run uglify-js",
    "bundle-brify": "onchange ./bundle-brify.js -v -- npm run bundle-parcelify ",
    "bundle-css": "onchange ./bundle-css.css -v -- npm run bundle-css-r",
    "bundle-css-r": "node r.js -o cssIn=bundle-css.css out=dist/minify/bundlecss.css",
    "build:browserify": "browserify src/shared/js/prueba29.js -g [partialify] -o bundle-brify-tmp.js -v -p [parcelify -wo bundle-css.css]",
    "build:browserify2": "browserify src/shared/js/prueba29.js -g [partialify] -o bundle-brify-tmp.js -v -p [parcelify -wo bundle-css.css]",
    "build:watchify": "watchify src/shared/js/prueba29.js -o bundle-brify.js -v -p [parcelify -wo bundle-css.css]",
    "build:watchify2": "watchify src/shared/js/prueba29.js  -g [partialify] -o bundle-brify.js -v -p [parcelify -wo bundle-css.css]",
    "liveserver": "live-server --port=8082 --watch=dist/minify --browser=firefox"
  },
  "author": "andres pallares lopez",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-runtime": "^6.20.0",
    "babelify": "^7.3.0",
    "bootstrap": "^3.3.7",
    "browser-sync": "^2.18.8",
    "browserify-css": "^0.10.1",
    "browserify-data": "^0.2.0",
    "concurrently": "^3.4.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "^3.0.4",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-filter": "^5.0.0",
    "gulp-flatten": "^0.3.1",
    "gulp-folders": "^1.1.0",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.2.0",
    "gulp-inject": "^4.2.0",
    "gulp-main-bower-files": "^1.6.2",
    "gulp-minify-css": "^1.2.4",
    "gulp-obfuscate": "^0.2.9",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-rev": "^7.1.2",
    "gulp-rimraf": "^0.2.1",
    "gulp-sass": "^3.1.0",
    "gulp-strip-comments": "^2.4.5",
    "gulp-strip-css-comments": "^1.2.0",
    "gulp-strip-debug": "^1.1.0",
    "gulp-uglify": "^2.1.2",
    "gulp-usemin": "^0.3.28",
    "gulp-useref": "^3.1.2",
    "gulp-util": "^3.0.8",
    "node-sass": "^4.5.3",
    "onchange": "^3.2.1",
    "parcelify": "^2.2.0",
    "partialify": "^3.1.6",
    "path": "^0.12.7",
    "require-dir": "^0.3.1",
    "run-sequence": "^1.2.2",
    "standard": "^10.0.2",
    "stringify": "^5.1.0",
    "uglify-js": "^3.0.15",
    "uglifyify": "^3.0.4",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "concurrently": "^3.4.0",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "handlebars": "^4.0.8",
    "inputmask": "^3.3.6",
    "jquery": "^2.2.4",
    "jquery-mousewheel": "^3.1.13",
    "linq.js": "^1.0.5",
    "moment": "^2.18.1",
    "mousetrap": "^1.6.1",
    "pikaday": "^1.5.1",
    "tether": "^1.4.0"
  },
  "stringify": {
    "appliesTo": {
      "includeExtensions": [
        ".html",
        ".htm"
      ]
    },
    "minify": true,
    "global": true
  },
  "browserify": {
    "transform": [
      "partialify"
    ]
  },
  "browserify-css": {
    "autoInject": true,
    "minify": true,
    "inlineImages": true,
    "rootDir": "."
  },
  "style": []
}
